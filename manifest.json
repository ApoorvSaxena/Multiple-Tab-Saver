{
  "name": "Simply Tabs Saver",
  "version": "1.0.2",
  "description": "Quickly save or restore all tabs with just a click!",
  "background": { "scripts": ["jquery.js", "background.js"] },
  "icons": { 
    "16": "icon16.png",
    "24": "icon24.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs"    
  ],
  "browser_action": {
    "name": "Quickly Save Tabs",    
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Save Tabs"
  },  
  "manifest_version": 2
}